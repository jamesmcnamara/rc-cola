[core]
    editor = vim
    
[color]
    ui = true

[diff]
    tool = vimdiff

[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    sh = stash
    pop = stash pop
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    po = push origin
    get = pull origin
